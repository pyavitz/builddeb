#!/bin/bash
### DO NOT EDIT
RED="\e[0;31m"
FIN="\e[0m"

DEFCONFIG=""
VERSION="5.10.32"
rc="0"
ARCH="x86"
GCC="gcc-8"
menuconfig="0"
rtl88XXau="0"
rtl88X2bu="0"
rtl88XXcu="0"

# open fd
exec 3>&1

# Store data to $VALUES variable
VALUES=$(dialog --ok-label "Submit" --no-cancel \
	  --backtitle "Linux kernel builder for Debian, Devuan and Ubuntu" \
	  --title "Config Menu" \
	  --form "Choose:" \
0 0 0 \
        "Defconfig:"   	1 1     "$DEFCONFIG"        	1 12 14 0 \
        "Branch:"   	2 1     "$VERSION"        	2 9 10 0 \
        "RC:"   	3 1     "$rc"	        	3 5 2 0 \
        "Arch:"   	4 1     "$ARCH"	        	4 7 7 0 \
        "GCC:"   	5 1     "$GCC"	        	5 6 7 0 \
	"Menuconfig:"	6 1	"$menuconfig"  		6 13 2 0 \
	"rtl88XXau:"	7 1	"$rtl88XXau"	 	7 13 2 0 \
	"rtl88X2bu:"	8 1	"$rtl88X2bu"	 	8 13 2 0 \
	"rtl88XXcu:"	9 1	"$rtl88XXcu"	 	9 13 2 0 \
2>&1 1>&3)

# close fd
exec 3>&-

# convert into vars
echo "$VALUES" > tmp1
echo DEFCONFIG='"' >> tmp0
echo VERSION='"' >> tmp0
echo rc= >> tmp0
echo ARCH='"' >> tmp0
echo GCC='"/usr/bin/' >> tmp0
echo menuconfig= >> tmp0
echo rtl8812au= >> tmp0
echo rtl88x2bu= >> tmp0
echo rtl8811cu= >> tmp0
echo '"' > tmp2
echo '"' >> tmp2
echo >> tmp2
echo '"' >> tmp2
echo '"' >> tmp2
paste -d '\0' tmp0 tmp1 tmp2  > userdata.txt
rm -f tmp0 tmp1 tmp2

# clear screen
clear

# userdata
echo -e "${RED}userdata file created${FIN}."
